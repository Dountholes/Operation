<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.operation">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.setting.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="com.example.operation.START_LASTACTIVITY" /> <!-- 相当于活动的代号 -->
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.setting.VideoBroadcast" />
        <activity android:name=".activity.login.GuideActivity" />
        <activity android:name=".activity.setting.DIY" />
        <activity android:name=".activity.setting.Song" />
        <activity android:name=".activity.setting.Style" />
        <activity android:name=".activity.setting.Timing"></activity>
    </application>

</manifest>